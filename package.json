    "rollup": "^3.28.1",
    "rollup-plugin-sass": "^1.12.20",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-swc": "^0.2.1",
    "unplugin-vue": "^4.3.5",
    "vite": "^4.4.9",
    "vue": "^3.3.4"
